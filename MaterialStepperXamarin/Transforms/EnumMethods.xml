<enum-method-mappings>
  
    <!--
  This example changes the Java method:
    android.support.v4.app.Fragment.SavedState.writeToParcel (int flags)
  to be:
    android.support.v4.app.Fragment.SavedState.writeToParcel (Android.OS.ParcelableWriteFlags flags)
  when bound in C#.
  
  path="/api/package[@name='com.stepstone.stepper']/class[@name='StepperLayout']/method[@name='setOrientation' and count(parameter)=1 and parameter[1][@type='int']]"
  
    
  <mapping jni-class="com/stepstone/stepper/stepperlayout">
    <method jni-name="setOrientation" parameter="orientation" clr-enum-type="Android.Widget.LinearLayout.Orientation" />
  </mapping>
  -->
  
  
</enum-method-mappings>